# Klang Example Program

say("Hello, world!")

let a = 5
if (a == 6 or 3 == 4) and (3 == 3 and not 5 == 6) then
	say(123)
elif a == 5 then
	say(456)
else
	say(789)
end

let a = []
for i = 1 to 20 step 2 do

	if i == 5 or i == 15 then
		continue
	end

	if i == 17 then
		break
	end

	append(a, i ^ 2)
end

say(a)

while len(a) do
	pop(a, 0)
end

say(a)

fun add(a, b)
	return a + b
end

fun mul(a, b) => a * b

say(add(2, 3))
say(mul(2, 3))