# Klang Example Program

say("Hello, world!")

let a = 5
if (a == 6 or 3 == 4) and (3 == 3 and not 5 == 6) then
	say(123)
else if a == 5 then
	say(456)
end

let a = []
for i = 0 to 10 then
	append(a, i ^ 2)
end
say(a)

let b = 0
while len(a) then
	say(b)
	b = b + 1
	if b == 3 then
		continue
	end

	if b == 7 then
		break
	end

	pop(a, 0)
end
say(a)

fun test(a, b)
	return a + b
end

say(test(2, 3))

